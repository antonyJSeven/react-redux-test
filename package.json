{
  "name": "em-luna",
  "version": "1.0.0",
  "description": "Eikon Messenger Luna",
  "main": "index.js",
  "scripts": {
    "css": "npx lessc src/stylesheets/main.less dist/css/main.css",
    "prebuild": "npm run validate",
    "build": "npm run babel && npm run bundle && npm run css",
    "babel": "npx babel src --out-dir temp/javascript",
    "bundle": "npx webpack --entry ./temp/javascript/app.js --output ./dist/app.js --mode development --devtool cheap-eval-source-map",
    "eslint": "eslint src/",
    "validate": "npm run eslint && npm run flow && npm run test",
    "flow": "npx flow",
    "test": "npx jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate",
      "pre-push": "npm run validate"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-react-app": "^3.1.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.87.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "less": "^3.9.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0"
  }
}
