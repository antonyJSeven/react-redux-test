{
  "name": "em-luna",
  "version": "1.0.0",
  "description": "Eikon Messenger Luna",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run validate",
    "build": "npm run babel && npm run bundle",
    "babel": "npx babel src --out-dir temp/javascript --presets react-app/prod",
    "bundle": "npx webpack --entry ./temp/javascript/app.js --output ./dist/app.js --mode development --devtool cheap-eval-source-map",
    "eslint": "eslint src/",
    "validate": "npm run eslint && npm run flow",
    "flow": "flow",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-react-app": "^3.1.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.87.0",
    "husky": "^1.2.0",
    "less": "^3.9.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
